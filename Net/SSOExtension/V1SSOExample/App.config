    <!-->
		<add key="v1ServerUrl" value="https://{VersionOne Server}/{VersionOne Instance}"/>
    <add key="username" value=""/>
    <add key="password" value=""/>
    <add key="integrated" value="true"/>
<!-->
    <!--
		The idpUrl parameter needs to contain the URL to your Identity Provider (IDP) and the parameters necessary for your IDP to perform the following actions:
			1. know which Service provider to authenticate on behalf of (versionone.com)
			2. stuff the Applicaiton URL into the RelayState (TargetResource)

		For our testing we used Ping Identity, so our idpUrl value looked like:
			http://{idp server}/idp/startSSO.ping?PartnerSpId=versionone.com&amp;TargetResource=https://{VersionOne Server}/{VersionOne Instance}/Default.aspx
			
		Unless you are using Ping Identity, your parameters could change.
		<add key="idpUrl" value=""/>
	-->
    <add key="idpUrl" value="http://sso:8081/idp/startSSO.ping?PartnerSpId=versionone.com&amp;TargetResource=https://www7.v1host.com/V1SSOTest/default.aspx" />
    <add key="idpResponseParser" value="V1SSOExample.sso.providers.PingIdpResponseParser,V1SSOExample" />
    <add key="spResponseParser" value="V1SSOExample.sso.providers.PingSpResponseParser,V1SSOExample" />
  </appSettings>
</configuration>